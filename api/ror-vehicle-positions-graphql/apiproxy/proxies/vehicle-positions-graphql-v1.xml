<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ProxyEndpoint name="vehicle-positions-graphql-v1">
    <Description/>
    <FaultRules/>
    <Flows/>
    <PostFlow name="PostFlow">
        <Request/>
        <Response/>
    </PostFlow>
    <PreFlow name="PreFlow">
        <Request>
            <Step>
                <FaultRules/>
                <Name>flowCallout.globalIpBlacklistTeamRutedata</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>spikeArrest</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>flowCallout.getEnvironmentConfiguration</Name>
            </Step>
        </Request>
        <Response/>
    </PreFlow>
    <HTTPProxyConnection>
        <BasePath>/realtime/v1/vehicles</BasePath>
        <Properties/>
        <VirtualHost>entur</VirtualHost>
    </HTTPProxyConnection>
    <RouteRule name="vehicle-positions-graphql-v1">
        <TargetEndpoint>vehicle-positions-graphql-v1</TargetEndpoint>
    </RouteRule>
</ProxyEndpoint>